# =====================================
# Server Configuration
# =====================================

# The port number on which the Express server will listen.
PORT=5555

# =====================================
# MySQL Database Configuration
# =====================================

# Hostname or IP address of the MySQL server.
DB_HOST=localhost

# Username for authenticating with the MySQL database.
DB_USER=your_mysql_username

# Password for the specified MySQL user.
DB_PASSWORD=your_mysql_password

# Name of the MySQL database to connect to.
DB_NAME=intercepted_data_db

# Port number on which the MySQL server is listening.
# Default is 3306 for MySQL.
DB_PORT=3306
